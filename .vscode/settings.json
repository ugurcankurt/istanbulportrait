{
    "css.validate": false,
    "less.validate": false,
    "scss.validate": false,
    "tailwindCSS.experimental.classRegex": [
        [
            "className\\s*:\\s*['\"]([^'\"]*)['\"]",
            "([a-zA-Z0-9\\-:]+)"
        ],
        [
            "cn\\(([^)]*)\\)",
